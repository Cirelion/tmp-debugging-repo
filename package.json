{
  "type": "module",
  "name": "unstability",
  "private": true,
  "scripts": {
    "build": "next build",
    "deploy": "alchemy deploy",
    "destroy": "alchemy destroy",
    "dev": "alchemy dev",
    "lint": "eslint .",
    "ts-lint": "tsc --noEmit",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "start": "next start",
    "db:generate": "prisma generate --schema=prisma/schema-auth.prisma && prisma generate --schema=prisma/schema-primary.prisma",
    "db:auth:generate": "prisma generate --schema=prisma/schema-auth.prisma",
    "db:auth:push": "prisma db push --schema=prisma/schema-auth.prisma",
    "db:auth:studio": "prisma studio --schema=prisma/schema-auth.prisma",
    "db:primary:generate": "prisma generate --schema=prisma/schema-primary.prisma",
    "db:primary:push": "prisma db push --schema=prisma/schema-primary.prisma",
    "db:primary:studio": "prisma studio --schema=prisma/schema-primary.prisma"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.11.0",
    "@prisma/adapter-pg": "^6.18.0",
    "@prisma/client": "^6.18.0",
    "better-auth": "^1.3.32",
    "next": "15.5.6",
    "pg": "^8.16.3",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251014.0",
    "@types/node": "^24.9.1",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "alchemy": "0.76.1",
    "eslint": "^9.38.0",
    "eslint-config-next": "15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "typescript": "^5.9.3",
    "wrangler": "^4.45.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "tsc --noEmit",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "prettier --write"
    ]
  }
}
